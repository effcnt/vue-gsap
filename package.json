{
  "name": "vue-gsap",
  "description": "Declarative GSAP for your Vue applications",
  "version": "0.0.1-0",
  "license": "MIT",
  "repository": "https://github.com/effcnt/vue-gsap",
  "homepage": "vue-gsap.effcnt.dev",
  "scripts": {
    "build": "rm -rf dist && vue-tsc && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "release": "pnpm build && release-it"
  },
  "peerDependencies": {
    "gsap": "^3.9.0",
    "vue": "^2.7.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/repl": "^1.5.0",
    "gsap": "^3.11.5",
    "release-it": "^15.10.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitepress": "^1.0.0-alpha.73",
    "vue-tsc": "^1.4.4",
    "vue-gsap": "link:./"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "module": "./dist/vue-gsap.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-gsap.es.js"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
